        enum Tokens{
                IDENTIFIER=0x100,
        	    VOID,
        	    INT,
        	    FLOAT,
                IF,
                ELSE,
                WHILE,
                FOR,
                RETURN,
                IDENTIFIER,
                OR_OP,
                AND_OP,
                EQ_OP,
                LE_OP,
                GE_OP,
                INC_OP,
                INT_CONSTANT,
        	    FLOAT_CONSTANT,
        	    STRING_LITERAL,
                    };
